call = paste(deparse(formula), ", ", "data= ", substitute(data),
sep = "")
f = formula
myt = terms(f, data = data)
x = as.character(f[[3]])
x = unlist(strsplit(x, "+", fixed = TRUE))
cat("x=",x,"\n")
str(x)
y = as.character(f[[2]])
cat("y=",y,"\n")
str(y)
}
